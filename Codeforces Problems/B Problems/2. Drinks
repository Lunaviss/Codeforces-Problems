#include <bits/stdc++.h>
 
using namespace std;
 
void solve() {
	int n;
	cin >> n;
	int a;
	double sum = 0;
	for(int i = 0; i < n; ++i) {
		cin >> a;
		sum += a;
	}
	cout << sum/n;
}
 
int main(int argc, char const *argv[])
{
	ios_base::sync_with_stdio(false);
	cin.tie(0);
	// freopen("learning.INP", "r", stdin);
	solve();
	return 0;
}
